{
  "name": "careertutor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "aws-deploy": "npm run build && aws s3 sync dist/ s3://careertutor.fractalslab.com --delete --cache-control max-age=0",
